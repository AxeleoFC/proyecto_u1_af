package com.uce.edu.demo.service;

import org.springframework.stereotype.Service;

import com.uce.edu.demo.modelo.Materia;

@Service
public class MateriaServiceImpl implements IMateriaService {
	
	private IMateriaService materiaService;

	@Override
	public void ingresarMateria(Materia m) {
		// TODO Auto-generated method stub
		this.materiaService.ingresarMateria(m);
	}

	@Override
	public void buscarMateria(String nombre) {
		// TODO Auto-generated method stub
		this.materiaService.buscarMateria(nombre);
	}

	@Override
	public void actualizarMateria(Materia m) {
		// TODO Auto-generated method stub
		this.materiaService.actualizarMateria(m);
	}

	@Override
	public void borrarMateria(String nombre) {
		// TODO Auto-generated method stub
		this.materiaService.borrarMateria(nombre);
	}

}
